444417722           # 72 -> 4! * 3
D                   # output H
73                  # set back to 0
4444177722244444    # 101 -> 4! * 4 + 5
D                   # output e
4444444             # 108 -> 101 + 7
DD                  # output ll
444                 # 111 -> 108 + 3
D                   # output o
73                  # set back to 0
444415572           # 44 -> (4! - 2) * 2
d                   # output ,
73                  # set back to 0
4472727272          # 32 -> 2*2*2*2*2
D                   # output space
77722555555555      # 87 -> 32 * 3 - 9 (with 32 under)
D                   # output W
E                   # pop 87, get back to 32
444447722           # 111 -> (32 + 5) * 3
D                   # output o
444                 # 114 -> 111 + 3
D                   # output r
555555              # 108 -> 114 - 6
D                   # output l
55555555            # 100 -> 108 - 8
D                   # output d
73                  # set back to 0
44727272724         # 33 -> 2*2*2*2*2 + 1
D                   # output !